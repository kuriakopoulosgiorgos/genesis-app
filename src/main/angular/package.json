{
  "name": "genesis",
  "version": "0.0.0",
  "scripts": {
    "generate:schema": "ng-openapi-gen --input ../../../target/generated/openapi.json --output src/app/api",
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "mock:server": "nodemon mock-server/server.js --watch mock-server/*",
    "start:proxy": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --ssl --ssl-key ../resources/dev-ssl-certificate/key.pem  --ssl-cert ../resources/dev-ssl-certificate/cert.pem",
    "build": "ng build",
    "build-prod": "npm run generate:schema && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/localize": "^10.0.14",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "babylonjs": "^4.2.0",
    "babylonjs-gui": "^4.2.0",
    "babylonjs-loaders": "^4.2.0",
    "bootstrap": "^4.5.3",
    "earcut": "^2.2.2",
    "jquery": "^3.5.1",
    "save": "^2.4.0",
    "tslib": "^2.0.0",
    "webxr-polyfill": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.3",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.7",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "ng-openapi-gen": "^0.17.3"
  }
}
